{
    "compilerOptions": {
        "module": "ESNext",
        "lib": [
            "dom",
            "ES2017"
        ],
        "moduleResolution": "node",
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "target": "ES2017",
        "noImplicitAny": true,
        "strictFunctionTypes": true,
        "strictBindCallApply": true,
        "noImplicitThis": true,
        "noImplicitReturns": true,
        "sourceMap": true
    }
}